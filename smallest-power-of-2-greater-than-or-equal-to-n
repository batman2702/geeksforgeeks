def nextpow(n):
    if(not(n and(n-1))):
        return n
    p=1 
    while(p<n):
        p=p<<1 
    return p
n=int(input())
print nextpow(n)



#include<bits/stdc++.h>
using namespace std;
int nextpow(int n)
{
    if(!(n&(n-1)))
    {
        return n;
    }
    int p=1;
    while(p<n)
    {
        p<<=1;
    }
    return p;
}
int main()
{
    int n;
    cin>>n;
    cout<<nextpow(n)<<endl;
}
